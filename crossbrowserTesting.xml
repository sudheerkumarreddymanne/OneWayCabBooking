<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
	
	<groups>
		<run>
		<!--	<include name="Regression"/> -->
			<include name="Master"/> 
		<!--	<include name="Smoke"/> -->
		</run>
	</groups>
	<listeners>
		<listener class-name="userDefinedLibraries.ExtentReportManager"/>
	</listeners>
	
  	<test thread-count="5" name="Test_1">
		<parameter name="os" value="windows"></parameter>
		<parameter name="browser" value="edge"></parameter>
    	<classes>
   			<class name="Tests.TC001_CabBookingTest"/>
   			<class name="Tests.TC002_GiftCardTest"/> 
   			<class name="Tests.TC003_HotelSelectionTest"/> 
   			<class name="Tests.TC004_HomePageVerification"/>
    </classes>
    </test> <!-- Test -->
 
  	<test thread-count="5" name="Test_2">
		<parameter name="os" value="windows"></parameter>
		<parameter name="browser" value="chrome"></parameter>
    	<classes>
   			<class name="Tests.TC001_CabBookingTest"/>
   			<class name="Tests.TC002_GiftCardTest"/> 
   			<class name="Tests.TC003_HotelSelectionTest"/> 
   			<class name="Tests.TC004_HomePageVerification"/>
    </classes>
    </test> <!-- Test -->
  
</suite> <!-- Suite -->
